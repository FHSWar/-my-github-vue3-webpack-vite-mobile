{
	"name": "createvue",
	"version": "1.0.0",
	"description": "none",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"anls": "node ./script/erase-html.cjs && ANALYZE=true webpack --config build/webpack/webpack.prod.cjs",
		"dev": "node ./script/erase-html.cjs && vite",
		"dev:webpack": "node ./script/erase-html.cjs && NODE_ENV=development webpack serve --progress --hot --config build/webpack/webpack.dev.cjs",
		"build": "node ./script/erase-html.cjs && NODE_ENV=production webpack --config build/webpack/webpack.prod.cjs",
		"build:vite": "node ./script/erase-html.cjs && vue-tsc --noEmit && vite build",
		"lint": "eslint --fix --ext .cjs,.mjs,.ts,.tsx,.vue ./ --ignore-pattern dist/ --ignore-pattern node_modules/ --ignore-pattern public/",
		"lintcss": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
		"precommit": "lint-staged",
		"prepare": "husky",
		"prettier": "prettier --write \"src/**/*.{css,sass,cjs,js,json,ts,tsx,vue}\"",
		"preview": "vite preview"
	},
	"lint-staged": {
		"src/**/*.{cjs,js,ts,tsx,vue}": [
			"prettier --write",
			"npm run lint"
		],
		"*.{html,vue,css,sass,scss,less}": [
			"npm run lintcss"
		]
	},
	"keywords": [
		"create",
		"vue"
	],
	"author": "DUJINHUA506",
	"license": "MIT",
