{
	"name": "createvue",
	"version": "1.0.0",
	"description": "none",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"anls": "ANALYZE=true webpack --config build/webpack/webpack.prod.cjs",
		"dev": "vite --force",
		"dev:webpack": "NODE_ENV=development webpack serve --progress --hot --config build/webpack/webpack.dev.cjs",
		"build": "NODE_ENV=production webpack --config build/webpack/webpack.prod.cjs",
		"build:vite": "vue-tsc --noEmit && vite build",
		"lint": "eslint --fix --ext .ts,.vue src",
		"lintcss": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
		"precommit": "lint-staged",
		"prepare": "husky",
		"preview": "vite preview"
	},
	"lint-staged": {
		"src/**/*.{js,json,css,vue}": [
			"prettier --write",
			"npm run lint"
		],
		"*.{html,vue,css,sass,scss,less}": [
			"npm run lintcss"
		]
	},
	"keywords": [
		"create",
		"vue"
	],
	"author": "DUJINHUA506",
